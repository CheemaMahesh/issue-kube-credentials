generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Credential {
  id           String   @id @map("_id") @default(auto()) @db.ObjectId
  credentialId String   @unique
  name         String
  workerId     String
  issuedAt     DateTime @default(now())
}
